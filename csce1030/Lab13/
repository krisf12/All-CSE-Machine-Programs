#include<stdio.h>
#include<string.h>

int main(int argc, char *argv[])
{
if(argc <3 && argc>1)
printf("\nthere are no arguments\n\n");

if(argc==3 && strcmp(argv[1], "reverse")==0)
printf("%s\n", argv[2]);

if(argc>3 && strcmp(argv[1], "reverse")==0)
printf("%s\t%s\n", argv[2],argv[3]);


return 0;
}
